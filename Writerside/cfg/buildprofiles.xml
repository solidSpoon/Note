<buildprofiles xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd">
    <variables>
        <offline-docs>true</offline-docs>
        <primary-color>metallic</primary-color>
        <custom-favicons>favicon.png</custom-favicons>
        <header-logo>my-logo.png</header-logo>
        <product-web-url>https://solidspoon.xyz/note/</product-web-url>
    </variables>
    <footer>
        <social type="blog" href="https://solidspoon.xyz"/>
        <link href="https://github.com/solidSpoon/note">Edit on GitHub</link>
    </footer>
    <build-profile instance="note">
        <variables>
            <noindex-content>false</noindex-content>
        </variables>
    </build-profile>
</buildprofiles>